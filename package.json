{
  "name": "quick-credit",
  "version": "1.0.0",
  "description": "quick-credit",
  "main": "app.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development npm run migration && cross-env NODE_ENV=development nodemon server/app.js --exec babel-node",
    "migration": "babel-node server/db/migration",
    "build": "babel server --out-dir dist",
    "start": "babel-node server/app.js",
    "test": "cross-env NODE_ENV=test npm run migration && cross-env NODE_ENV=test nyc mocha --timeout 20000 --exit server/tests --require @babel/register --require babel-polyfill",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run migration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rythae/quick-credit.git"
  },
